{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/outbound-call",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/outbound-call.js"
    },
    {
      "src": "/api/webhook/call-ended",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/webhook/call-ended.js"
    },
    {
      "src": "/api/test-sms",
      "dest": "/api/test-sms.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
